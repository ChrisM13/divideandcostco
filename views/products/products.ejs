<h1>All Products</h1>
    <div class='list-group'>
        <% products.forEach(function(product){ %>
            <tr>
                <td><%= product.name %></td>
                <td>Quantity: <%= product.quantity %> </td>
                <td> Price $<%= product.price %></td>
                <br>
            </tr>
       <% }); %>



</div>
