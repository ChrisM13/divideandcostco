<h1>All Products</h1>
    <div>
        <table class="highlight">
            <thead>
                <tr>
                <th>Product Name</th>
                    <th>Quantity</th>
                    <th>Price</th>
                </tr>
            </thead>
            <tbody>
        <% products.forEach(function(product){ %>
            <tr>
                <td><%= product.name %></td>
                <td>Quantity: <%= product.quantity %> </td>
                <td> Price $<%= product.price %></td>
            </tr>
       <% }); %>
            </tbody>
        </table>
</div>
