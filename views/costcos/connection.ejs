<%- include('./../_partials/head') %>

<% matched.forEach(item => { %>
    <p><%= item %></p>
<% }) %>
    <!-- Other User List-->
    <div class="container">
        <div class="col s10">
            <table class="highlight">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Quantity</th>
                        <th>Price</th>
                    </tr>
                </thead>
                <tbody>
                    <% otherList.products.forEach(item => { %>
                        <tr>
                            <td>
                                <%=item.name%>
                            </td>
                            <td>
                                <%=item.quantity%>
                            </td>
                            <td>$
                                <%=item.price%>
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>
    </div>

    <!-- My List-->
    <div class="container">
        <div class="col s10">
            <table class="highlight">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Quantity</th>
                        <th>Price</th>
                    </tr>
                </thead>
                <tbody>
                    <% userList.products.forEach((item) => { %>
                        <tr>
                            <td>
                                <%=item.name%>
                            </td>
                            <td>
                                <%=item.quantity%>
                            </td>
                            <td>$
                                <%=item.price%>
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>
    </div>
    </div>
    </div>

